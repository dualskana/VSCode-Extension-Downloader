{
  "manifest_version": 3,
  "name": "VSCode Extension Downloader",
  "version": "1.0",
  "description": "在 VS Code Marketplace 的插件历史版本页面添加下载按钮",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://marketplace.visualstudio.com/*"],
      "js": ["src/content.js"],
      "run_at": "document_idle"
    }
  ]
}